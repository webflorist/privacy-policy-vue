{
	"name": "@webflorist/privacy-policy-vue",
	"description": "A GDPR compliant privacy policy component for Vue applications in german and english",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/webflorist/privacy-policy-vue.git"
	},
